---
import Layout from '../../layouts/Layout.astro';
import { HeroResume } from './_components/hero';
import { BentoGrid } from '$components/bento';

import { Skills } from './_components/skills';
import { AboutMeResume } from 'src/pages/resume/_components/about-me';
import { ResumeProjects } from 'src/pages/resume/_components/projects';
import { ResumeContact } from 'src/pages/resume/_components/contact';
import { sendCustomEvent } from '$lib/gtag';

export const prerender = true;

sendCustomEvent('pageview', '/resume', 'Resume', 'pageview');
---

<Layout title="Franciszek Stodulski - Software Developer">
  <HeroResume />

  <BentoGrid class="container print:py-4 py-10 auto-rows-min">
    <AboutMeResume />
    <ResumeContact />
    <Skills />
    <ResumeProjects />
  </BentoGrid></Layout
>
